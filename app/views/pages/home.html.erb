<h1> Home Page </h1>

<h2> Welcome to ReelHub </h2>
<ul>
	<li> 
		<% if user_signed_in? %>
				<%= link_to 'Feeds Page', user_feeds_path(current_user.id) %>
			<% else %>
				<%= link_to 'Feeds Page (Log In to use)', root_path %>
		<% end %>
	</li>
	<li> <%= link_to 'Videos Page', videos_path %> </li>
	<li> <%= link_to 'Artists Page', artists_path %> </li>
	<li><%= link_to 'About Page', about_path %></li>
	<li><%= link_to 'Support Page', support_path %></li>
	<li><%= link_to 'Home Page', root_path %></li>
</ul>

<% if user_signed_in? %>
	<p>
		Hello, <%= current_user.email %>
	</p>
    <%= link_to 'Sign Out', destroy_user_session_path %>
<% else %>
    <%= link_to 'Log In', new_user_session_path %> Don't have an account yet? <%= link_to 'Sign Up', new_user_registration_path %>
<% end %>